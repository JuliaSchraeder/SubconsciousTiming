    "  # Functions to extract reaction times\n",
    "  \n",
    "- reaction time can be checked in \"key_resp_study.rt\"\n",
    "  \n",
    "- 1 frame = 8 ms\n",
    "- 2 frames = 16 ms\n",
    "- 3 frames = 25 ms\n",
    "- 17 frames = 141 ms"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 52,
   "metadata": {},
   "outputs": [],
   "source": [
    "def reactionnumber(participantcsv, vp_number):\n",
    "    reaction_number = len(reaction_time)                               # number of reactions without missing values\n",
    "    \n",
    "    return reaction_number                                             # returned variable can be stored in df"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 53,
   "metadata": {},
   "outputs": [],
   "source": [
    "def reactionaverage(participantcsv, vp_number):\n",
    "    reaction_average = sum(reaction_time)/len(reaction_time)           # average of rt\n",
    "    \n",
    "    return reaction_average                                            # returned variable can be stored in df"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 54,
   "metadata": {},
   "outputs": [],
   "source": [
    "# write all happy reactions from each participant in df_p to create df_p with all reactions for each participant\n",
    "# returns mean reaction time for happy responses to write file with mean reaction times\n",
    "def reactionhappy(participantcsv, vp_number):\n",
    "    # Take just reactions to happy emotions #\n",
    "    reaction_happy = df_emotion[(df_emotion['emotion'] == 'happy')]\n",
    "    #just take reaction times from happy condition\n",
    "    justhappyreactiontimes = reaction_happy['reaction time'].dropna() # to drop trials with no reaction\n",
    "    #define mean reaction times for responses of happy faces\n",
    "    reaction_average_happy = sum(justhappyreactiontimes)/len(justhappyreactiontimes) \n",
    "    # write file with all reactions to happy emotions in list for every participant (df_reactions)\n",
    "    happy = justhappyreactiontimes\n",
    "    df_reactions['happy'] = happy \n",
    "    \n",
    "    # return average to store this later in df \n",
    "    return reaction_average_happy                                     "
   ]
  },
  {
   "cell_type": "code",
