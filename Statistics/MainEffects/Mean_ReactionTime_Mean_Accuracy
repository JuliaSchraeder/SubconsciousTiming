    "  # Functions to extract Accuracy\n",
    "\n",
    "- accuracy can be checked in column \"key_resp_study.corr\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 61,
   "metadata": {},
   "outputs": [],
   "source": [
    "# how many answers where correct?  in %\n",
    "# need dropna to delete NANs to get correct number of correct answers\n",
    "def correct(participantcsv, vp_number):\n",
    "    corr_ans_drop = participantcsv['key_resp_study.corr'].dropna()   # get list of correct answers\n",
    "    accuracy = sum(corr_ans_drop)/len(corr_ans_drop)* 100            # get percentage of correct answers\n",
    "    \n",
    "    return accuracy"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 62,
   "metadata": {},
   "outputs": [],
   "source": [
    "# how many answere where correct? in total\n",
    "# need dropna to delete NANs to get correct number of correct answers\n",
    "def numbercorrans(participantcsv, vp_number):\n",
    "    corr_ans_drop = participantcsv['key_resp_study.corr'].dropna()  # get list of correct answers                           \n",
    "    number_corr_ans_nonformatted = sum(corr_ans_drop) \n",
    "    number_corr_ans = \"{:.0f}\".format(number_corr_ans_nonformatted) # format value to not show .0\n",
    "    \n",
    "    return number_corr_ans"
   ]
  },
