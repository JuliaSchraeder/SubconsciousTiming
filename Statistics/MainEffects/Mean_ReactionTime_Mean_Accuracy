    "# Replace keypress (a,s,d) with emotion name\n",
    "hddm1 = hddm.replace(\"a\", \"sad\").replace(\"s\",\"neutral\").replace(\"d\",\"happy\")\n",
    "hddm1['level'] = hddm1['level'].replace([3.0],'25ms')\n",
    "hddm1['level'] = hddm1['level'].replace([2.0],'16ms')\n",
    "hddm1['level'] = hddm1['level'].replace([1.0],'8ms')\n",
    "hddm1['level'] = hddm1['level'].replace([17.0],'141ms')\n",
