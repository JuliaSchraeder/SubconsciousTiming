   "outputs": [],
   "source": [
    "def reactionsad(participantcsv, vp_number):\n",
    "    # Take just reactions to sad emotions #\n",
    "    reaction_sad = df_emotion[(df_emotion['emotion'] == 'sad')]\n",
    "    #just take reaction times from happy condition\n",
    "    justsadreactiontimes = reaction_sad['reaction time'].dropna()\n",
    "    #define mean reaction times for responses of sad faces\n",
    "    reaction_average_sad = sum(justsadreactiontimes)/len(justsadreactiontimes)\n",
    "    sad = justsadreactiontimes\n",
    "    df_reactions['sad'] = sad\n",
    "    \n",
    "    return reaction_average_sad"
   ]
